{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/formations-excel/le-decollage",
      "destination": "/formations-excel/le-decollage-liste-attente",
      "permanent": true
    },
    {
      "source": "/formations-excel/la-machine",
      "destination": "/formations-excel/la-machine-liste-attente",
      "permanent": true
    },
    {
      "source": "/formations-excel/la-slide",
      "destination": "/formations-excel/la-slide-liste-attente",
      "permanent": true
    },
    {
      "source": "/formations-excel/power-query-secrets",
      "destination": "/formations-excel/power-query-secrets-liste-attente",
      "permanent": true
    },
    {
      "source": "/blog",
      "destination": "/blog-excel",
      "permanent": true
    },
    {
      "source": "/blog/",
      "destination": "/blog-excel",
      "permanent": true
    },
    {
      "source": "/blog/:slug",
      "destination": "/:slug",
      "permanent": true
    },
    {
      "source": "/blog/:slug/",
      "destination": "/:slug",
      "permanent": true
    },
    {
      "source": "/category/:slug",
      "destination": "/categorie/:slug",
      "permanent": true
    },
    {
      "source": "/category/:slug/",
      "destination": "/categorie/:slug",
      "permanent": true
    },
    {
      "source": "/tag/:slug",
      "destination": "/categorie/:slug",
      "permanent": true
    },
    {
      "source": "/tag/:slug/",
      "destination": "/categorie/:slug",
      "permanent": true
    },
    {
      "source": "/formations/:slug",
      "destination": "/formations-excel/:slug",
      "permanent": true
    },
    {
      "source": "/formations/:slug/",
      "destination": "/formations-excel/:slug",
      "permanent": true
    },
    {
      "source": "/formations",
      "destination": "/formations-excel",
      "permanent": true
    },
    {
      "source": "/formations/",
      "destination": "/formations-excel",
      "permanent": true
    }
  ]
}
